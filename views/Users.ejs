<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Chats</title>
</head>
<body>
    <h3>All Users</h3>
    <!-- <button>Create Post</button> -->
    <% for (let user of allUsers) { %>
        <div>
            <p><%= user.name %></p>
            <p><%= user.email %></p>
            <p><%= user.password %></p>
            <!-- <p><%= chat.created_at %></p> -->
            <br>
        </div>
    <% } %>
</body>
</html>